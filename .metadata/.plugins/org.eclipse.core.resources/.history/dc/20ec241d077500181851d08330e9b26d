package searching;

import java.util.Scanner;

public class RestInPeace {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		if(t>=1 && t<=100) {
			for(int i = 0; i < t; i++) {
				int n = sc.nextInt();
				if(n>=1 && n<=1000000) {
					if(n%21 == 0) {
						System.out.println("The streak is broken!");
					}
				}
			}
		}
		
	}

}
