package binarySearch;

import java.util.Scanner;

public class BishuandSoldiers {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		if(N >=1 && N <= 10000) {
			int[] soldiers = new int[N];
			for(int i = 0; i < N; i++) {
				soldiers[i] = sc.nextInt();
			}
			int Q = sc.nextInt();
		}
		
	}

}
